package br.com.scraping.model;

import java.io.Serializable;
import java.util.ArrayList;

public class Noticia implements Serializable{

	private static final long serialVersionUID = 9203790109988278580L;

	private ArrayList<String> canalAbertura;
	private ArrayList<String> titulo;
	private ArrayList<String> link;
	private ArrayList<String> hora;

	public Noticia() {

	}
	
	public Noticia(ArrayList<String> canalAbertura, ArrayList<String> titulo, ArrayList<String> link,
			ArrayList<String> hora) {
		super();
		this.canalAbertura = canalAbertura;
		this.titulo = titulo;
		this.link = link;
		this.hora = hora;
	}

	public ArrayList<String> getCanalAbertura() {
		return canalAbertura;
	}

	public void setCanalAbertura(ArrayList<String> canalAbertura) {
		this.canalAbertura = canalAbertura;
	}

	public ArrayList<String> getTitulo() {
		return titulo;
	}

	public void setTitulo(ArrayList<String> titulo) {
		this.titulo = titulo;
	}

	public ArrayList<String> getLink() {
		return link;
	}

	public void setLink(ArrayList<String> link) {
		this.link = link;
	}
	
	public ArrayList<String> getHora() {
		return hora;
	}

	public void setHora(ArrayList<String> hora) {
		this.hora = hora;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((titulo == null) ? 0 : titulo.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Noticia other = (Noticia) obj;
		if (titulo == null) {
			if (other.titulo != null)
				return false;
		} else if (!titulo.equals(other.titulo))
			return false;
		return true;
	}
	
	@Override
	public String toString() {
		return "Canais de abetura : " + canalAbertura + 
				"\n"+
				"Titulos : " + titulo + 
				"\n"+
				"Links : " + link +
				"\n"+
				"Hora : " + hora;
	}

}
