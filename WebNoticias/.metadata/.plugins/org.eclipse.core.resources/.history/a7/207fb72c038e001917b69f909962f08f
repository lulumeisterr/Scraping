package br.com.scraping.model;

import java.io.Serializable;
import java.util.HashSet;

public class Noticia implements Serializable{

	private static final long serialVersionUID = 9203790109988278580L;

	private HashSet<String> canalAbertura;
	private HashSet<String> titulo;
	private HashSet<String> link;
	private HashSet<String> hora;

	public Noticia() {

	}

	public Noticia(HashSet<String> canalAbertura, HashSet<String> titulo, HashSet<String> link, HashSet<String> hora) {
		super();
		this.canalAbertura = canalAbertura;
		this.titulo = titulo;
		this.link = link;
		this.hora = hora;
	}

	public HashSet<String> getCanalAbertura() {
		return canalAbertura;
	}

	public void setCanalAbertura(HashSet<String> canalAbertura) {
		this.canalAbertura = canalAbertura;
	}

	public HashSet<String> getTitulo() {
		return titulo;
	}

	public void setTitulo(HashSet<String> titulo) {
		this.titulo = titulo;
	}

	public HashSet<String> getLink() {
		return link;
	}

	public void setLink(HashSet<String> link) {
		this.link = link;
	}

	public HashSet<String> getHora() {
		return hora;
	}

	public void setHora(HashSet<String> hora) {
		this.hora = hora;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((titulo == null) ? 0 : titulo.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Noticia other = (Noticia) obj;
		if (titulo == null) {
			if (other.titulo != null)
				return false;
		} else if (!titulo.equals(other.titulo))
			return false;
		return true;
	}
	
	@Override
	public String toString() {
		return super.toString();
	}

}
